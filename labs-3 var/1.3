#include<iostream>
#include<cmath>


using namespace std;

int main()
{
    double xn,xk,dx, a,b,c;
    cin >> xn >> xk >> dx >> a >> b >> c ;
    if(int(a) & (int(b) | int (c)) != 0) {
       for(double x = xn; x <= xk; x += dx) {
           double f;
           if(a < 0 && c != 0) f = a*pow(x,2)+b*x+c;
           else if (a > 0 && c == 0) f = -a/x-c;
           else f = a(x+c);
           count << "x = " << x << "\tf = " << f << endl;
       }
     }
     else {
          for(double x = xn; x <= xk; x += dx) {
                int f;
                if(a < 0 && c != 0) f = a*pow(x,2)+b*x+c;
                else if (a > 0 && c == 0) f = -a/x-c;
                else f = a(x+c);
                count << "x = " << x << "\tf = " << f << endl;
           }
       }
       return 0
}
